{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import"
  ],
  "settings": {
    "import/ignore": [
      "node_modules"
    ],
    "import/resolve": {
      "moduleDirectory": [
        "node_modules",
        "src"
      ]
    }
  }
}
